<?xml version="1.0"  encoding="UTF-8"?><?xml-stylesheet href="style.css" type="text/css"?><dialog   xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul"  xmlns:html="http://www.w3.org/1999/xhtml"   xmlns:svg="http://www.w3.org/2000/svg"   xmlns:xuljet="http://www.xuljet.com/xuljet"  xmlns:phobos="http://smalltalkhub.com/#!/~PavelKrivanek/Phobos"    ondialogaccept="__processAction('ondialogaccept')"  ondialogcancel="__processAction('ondialogcancel')"  ondialogdisclosure="__processAction('ondialogdisclosure')"  ondialogextra1="__processAction('ondialogextra1')"  ondialogextra2="__processAction('ondialogextra2')"  ondialoghelp="__processAction('ondialoghelp')">  <script type="text/javascript">    window.addEventListener("load", __onWindowOpened, false);		function __onWindowOpened()	{	  if (window.arguments[1]) (window.arguments[1])(window);      window.actions = window.arguments[2] ? window.arguments[2] : {};	}		function __processAction(actionName)	{	  if (window.actions[actionName]) (window.actions[actionName])()	}  </script>	  <description hidden="true"  id="__start_windowComponent"/>  <description hidden="true"  id="__end_windowComponent"/>	</dialog>